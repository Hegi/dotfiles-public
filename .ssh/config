Host corporate.github.com
    Hostname github.com
    User git
    IdentityFile ~/.ssh/corporate.github
    IdentitiesOnly yes
    PasswordAuthentication no
    PubkeyAuthentication yes
    AddKeysToAgent yes

Host other-corporate.github.com
    Hostname github.com
    User git
    IdentityFile ~/.ssh/other-corporate.github
    IdentitiesOnly yes
    PasswordAuthentication no
    PubkeyAuthentication yes
    AddKeysToAgent yes
